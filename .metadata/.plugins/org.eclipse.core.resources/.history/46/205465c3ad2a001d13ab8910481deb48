package day1;

public class TicTacToe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char arr[][]= {{' ','|',' ','|',' '},{'-','+','-','+','-'},{' ','|',' ','|',' '},{'-','+','-','+','-'},{' ','|',' ','|',' '}};
		for(char row[]:arr) {
			for(char c:row) {
				System.out.print(c);
			}
			System.out.println();
		}

	}

}
